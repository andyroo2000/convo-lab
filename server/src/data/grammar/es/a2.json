{
  "language": "es",
  "level": "A2",
  "framework": "CEFR",
  "grammarPoints": [
    {
      "pattern": "preterite tense",
      "meaning": "simple past tense for completed actions",
      "usage": "Use to talk about actions that were completed at a specific time in the past",
      "example": "Ayer comí paella",
      "exampleTranslation": "Yesterday I ate paella"
    },
    {
      "pattern": "imperfect tense",
      "meaning": "past tense for habitual or ongoing actions",
      "usage": "Use to describe past habits, ongoing past actions, or background information",
      "example": "Cuando era niño, jugaba en el parque",
      "exampleTranslation": "When I was a child, I used to play in the park"
    },
    {
      "pattern": "preterite vs imperfect",
      "meaning": "choosing between two past tenses",
      "usage": "Preterite for completed actions, imperfect for habitual/ongoing past actions",
      "example": "Llovía cuando salí",
      "exampleTranslation": "It was raining when I left"
    },
    {
      "pattern": "present perfect",
      "meaning": "recent past or past with present relevance",
      "usage": "Use 'haber' + past participle for actions that just happened or affect the present",
      "example": "He comido tres veces hoy",
      "exampleTranslation": "I have eaten three times today"
    },
    {
      "pattern": "simple future",
      "meaning": "future actions or predictions",
      "usage": "Use future tense endings to talk about what will happen",
      "example": "Mañana iré al cine",
      "exampleTranslation": "Tomorrow I will go to the cinema"
    },
    {
      "pattern": "conditional",
      "meaning": "hypothetical situations or polite requests",
      "usage": "Use conditional to express what would happen or make polite requests",
      "example": "Me gustaría un café",
      "exampleTranslation": "I would like a coffee"
    },
    {
      "pattern": "imperative (tú)",
      "meaning": "informal commands",
      "usage": "Use to give orders or instructions to someone you address as 'tú'",
      "example": "Habla más despacio",
      "exampleTranslation": "Speak more slowly"
    },
    {
      "pattern": "imperative (usted)",
      "meaning": "formal commands",
      "usage": "Use to give polite orders or instructions",
      "example": "Hable más despacio, por favor",
      "exampleTranslation": "Please speak more slowly"
    },
    {
      "pattern": "negative commands",
      "meaning": "telling someone not to do something",
      "usage": "Use subjunctive forms for negative commands",
      "example": "No hables tan rápido",
      "exampleTranslation": "Don't speak so fast"
    },
    {
      "pattern": "direct object pronouns",
      "meaning": "replacing direct objects with pronouns",
      "usage": "Use lo, la, los, las to replace direct object nouns",
      "example": "¿El libro? Lo tengo",
      "exampleTranslation": "The book? I have it"
    },
