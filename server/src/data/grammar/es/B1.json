{
  "language": "es",
  "level": "B1",
  "framework": "CEFR",
  "grammarPoints": [
    {
      "pattern": "present subjunctive",
      "meaning": "expressing wishes, doubts, or emotions",
      "usage": "Use after verbs of emotion, doubt, desire, or impersonal expressions",
      "example": "Espero que vengas",
      "exampleTranslation": "I hope you come"
    },
    {
      "pattern": "imperfect subjunctive",
      "meaning": "subjunctive in past contexts",
      "usage": "Use in past or hypothetical situations",
      "example": "Si tuviera dinero, viajaría",
      "exampleTranslation": "If I had money, I would travel"
    },
    {
      "pattern": "si clauses (conditional)",
      "meaning": "if-then statements",
      "usage": "Use to express hypothetical situations and their consequences",
      "example": "Si llueve, me quedaré en casa",
      "exampleTranslation": "If it rains, I'll stay home"
    },
    {
      "pattern": "por vs para",
      "meaning": "distinguishing between two prepositions",
      "usage": "'Por' for cause/duration, 'para' for purpose/destination",
      "example": "Estudio para el examen",
      "exampleTranslation": "I study for the exam"
    },
    {
      "pattern": "passive voice with ser",
      "meaning": "passive constructions",
      "usage": "Use 'ser' + past participle to form passive voice",
      "example": "El libro fue escrito por Cervantes",
      "exampleTranslation": "The book was written by Cervantes"
    },
    {
      "pattern": "passive se",
      "meaning": "impersonal passive constructions",
      "usage": "Use 'se' + verb for general statements",
      "example": "Se habla español aquí",
      "exampleTranslation": "Spanish is spoken here"
    },
    {
      "pattern": "relative pronouns",
      "meaning": "connecting clauses with que, quien, cual",
      "usage": "Use to provide additional information about nouns",
      "example": "El hombre que vive allí es mi vecino",
      "exampleTranslation": "The man who lives there is my neighbor"
    },
    {
      "pattern": "pluperfect",
      "meaning": "past perfect tense",
      "usage": "Use to describe actions completed before another past action",
      "example": "Cuando llegué, ya habían salido",
      "exampleTranslation": "When I arrived, they had already left"
    },
    {
      "pattern": "present perfect subjunctive",
      "meaning": "subjunctive in present perfect",
      "usage": "Use to express doubt or emotion about past events",
      "example": "Dudo que hayan llegado",
      "exampleTranslation": "I doubt they have arrived"
    },
    {
      "pattern": "indirect speech",
      "meaning": "reporting what someone said",
      "usage": "Use to relay information from another source",
      "example": "Dijo que vendría",
      "exampleTranslation": "He said he would come"
    },
